		var number = value * value2;
		Console.WriteLine(number);
	}
	
	public void Compare (int value, int value2) {
		int x = 30;
		int y = 10;
		if (x > y) 
		if(value > value2){
			Console.WriteLine("The temperature is below 10 degrees Celsius, suggest wearing warm clothing.");
